<%= simple_form_for(experience) do |f| %>
  <%= f.input :name, label: 'Experience name' %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price, as: :string %>
  <%= f.input :duration, label: 'Duration (minutes)', as: :string %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Submit", class: 'btn btn-info mt-2' %>
<% end %>
